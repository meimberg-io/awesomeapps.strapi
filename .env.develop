# Environment Variables Example
# Copy this to .env for local development

# Application
NODE_ENV=development
APP_PORT=8202

# Database
DATABASE_CLIENT=mysql
DATABASE_HOST=localhost
DATABASE_PORT=3306
DATABASE_NAME=strapi
DATABASE_USERNAME=strapi
DATABASE_PASSWORD=strapi_password
DATABASE_ROOT_PASSWORD=root_password
DATABASE_SSL=false

# Strapi App Keys (required)
APP_KEYS=defaultKey1,defaultKey2,defaultKey3,defaultKey4
API_TOKEN_SALT=defaultSalt
ADMIN_JWT_SECRET=defaultSecret
TRANSFER_TOKEN_SALT=defaultTransferSalt
JWT_SECRET=defaultJWTSecret

# Development specific
DEBUG=true
LOG_LEVEL=debug

# Live Transfer (for pull-live script)
STRAPI_LIVE_URL=***
STRAPI_LIVE_TOKEN=***
