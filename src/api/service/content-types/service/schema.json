{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "abstract": {
      "type": "text"
    },
    "url": {
      "type": "string"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "services"
    },
    "longdescription": {
      "type": "blocks"
    },
    "screenshots": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "articlecontent": {
      "type": "dynamiczone",
      "components": [
        "shared.slider",
        "shared.rich-text",
        "shared.quote",
        "shared.media"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "pricing": {
      "type": "richtext"
    },
    "description": {
      "type": "richtext"
    },
    "functionality": {
      "type": "richtext"
    },
    "shortfacts": {
      "type": "text"
    },
    "youtube_video": {
      "type": "string"
    },
    "youtube_title": {
      "type": "string"
    },
    "publishdate": {
      "type": "date"
    },
    "top": {
      "type": "boolean"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "service"
    }
  }
}
